{

    "manifest_version": 2,
    "name": "Tome",
    "version": "0.3.5",

    "default_locale": "en",

    "description": "Search through things",
    "icons": {
        "48": "icons/history-search.png"
    },

    "permissions": [
        "bookmarks",
        "history",
        "<all_urls>",
        "webRequest",
        "http://*/",
        "https://*/"
    ],

    "browser_action": {
        "default_icon": {
            "32": "icons/history-search.png"
        },
        "default_title": "BookmarkSearch",
        "default_popup": "index.html"
    },

    "background": {
        "scripts": ["commons.js","background.js"]
    }

}